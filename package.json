{
  "name": "agenda",
  "version": "1.0.0",
  "engines": {
    "node": "7.10.0"
  },
  "scripts": {
    "prebuild": "rm -rf dist && mkdir dist",
    "build": "webpack -p --config webpack.prod.config.js",
    "start": "json-server --static ./dist --watch db.json --routes routes.json --port $PORT",
    "api:start": "json-server --watch db.json --routes routes.json",
    "dev:start": "webpack-dev-server --config webpack.dev.config.js"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.5.2",
    "compression-webpack-plugin": "^0.4.0",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "http-server": "^0.10.0",
    "json-server": "^0.10.1",
    "ngtemplate-loader": "^2.0.1",
    "purify-css": "^1.2.5",
    "purifycss-webpack": "^0.7.0",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.5.0"
  },
  "dependencies": {
    "angular": "^1.6.4",
    "lodash": "^4.17.4",
    "materialize-css": "^0.98.2"
  }
}
